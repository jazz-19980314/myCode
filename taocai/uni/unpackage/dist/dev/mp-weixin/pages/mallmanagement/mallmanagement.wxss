.bigcontainer {
  height: 100vh;
  background: #f5faff;
}
.busname {
  width: 100%;
  text-align: center;
  height: 44rpx;
  font-size: 16px;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 44rpx;
  margin-bottom: 5rpx;
}
.bususer {
  width: 100%;
  text-align: center;
  height: 34rpx;
  font-size: 12px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 34rpx;
}
/* 大盒子 */
.cu-list.menu-avatar {
  width: 690rpx;
  height: 160rpx;
  border-radius: 10px;
  margin: 20rpx auto;
  margin-top: 44rpx;
  position: relative;
}
.cu-list.menu-avatar .bg-image {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.cu-list.menu-avatar > .cu-item {
  height: 100%;
}
.cu-list.menu-avatar > .cu-item > .cu-avatar {
  width: 80rpx;
  height: 80rpx;
  left: 20rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
/* 头像 */
.tx-image {
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
}
/* 用户名盒子 */
.cu-list.menu-avatar > .cu-item .content {
  left: 130rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.cu-list.menu-avatar > .cu-item .content > .text-black {
  width: 248rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 44rpx;
  margin-bottom: 10rpx;
}
.cu-btn.sm {
  width: 144rpx;
  height: 36rpx;
  border-radius: 18rpx;
  border: 1px solid #FFFFFF;
  background-color: transparent;
  font-size: 10px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #fff;
  line-height: 36rpx;
}
/* 设置按钮 */
.cu-list.menu-avatar > .cu-item .action {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
  /* margin-bottom: 210rpx; */
  margin-left: 80rpx;
}
.icon-setting {
  height: 36rpx;
  width: 36rpx;
  position: absolute;
  top: 30rpx;
  right: 20rpx;
}
.normal {
  width: 690rpx;
  height: 464rpx;
  background: #feffff;
  box-shadow: -2px -2px 4px 0px rgba(229, 238, 255, 0.5), 2px 2px 4px 0px rgba(24, 79, 186, 0.4);
  border-radius: 10px;
  margin: 40rpx auto;
  padding: 30rpx;
}
.no-content, .no-title {
  font-size: 14px;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 20px;
  text-align: center;
}
.no-content .mainbox, .no-title .mainbox {
  width: 240rpx;
  height: 240rpx;
  background: #FEFFFF;
  box-shadow: -2px -2px 4px 1px white, 2px 2px 4px 1px rgba(24, 79, 186, 0.4);
  border-radius: 4px;
  position: relative;
}
.no-content .mainbox text, .no-title .mainbox text {
  width: 44rpx;
  height: 50rpx;
  font-size: 18px;
  font-family: PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  line-height: 50rpx;
  position: absolute;
  bottom: 60rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.no-content .mainbox image, .no-title .mainbox image {
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.no-navbar {
  margin-top: 60rpx;
  width: 100%;
  padding: 10rpx 0;
  font-size: 14px;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #333333;
}
.no-navbar .no-top {
  width: 100%;
  height: 70rpx;
  padding: 0 40rpx 0 60rpx;
  margin-bottom: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.no-navbar .no-top .no-left {
  padding-left: 60rpx;
  width: 180rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.no-navbar .no-top .no-left image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  left: 0;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}

