.main {
  width: 100%;
  padding: 40rpx 30rpx;
}
.main .main-search {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.main .main-search .cuIcon-search {
  position: absolute;
  top: 50%;
  left: 40rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.main .main-search input {
  width: 560rpx;
  height: 72rpx;
  padding-left: 80rpx;
  background: #feffff;
  box-shadow: -2px -2px 4px 0px rgba(227, 238, 255, 0.5), 2px 2px 4px 0px rgba(0, 80, 193, 0.4);
  border-radius: 4px;
}
.main .main-search .search-right {
  height: 72rpx;
  width: 110rpx;
  line-height: 72rpx;
  padding-right: 40rpx;
  font-size: 16px;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #333333;
  position: relative;
}
.main .main-search .search-right image {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  right: 0rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.main .signFor {
  width: 690rpx;
  height: 400rpx;
  margin: 50rpx auto;
  background: #feffff;
  box-shadow: -2px -2px 4px 0px rgba(227, 238, 255, 0.5), 2px 2px 4px 0px rgba(0, 80, 193, 0.2);
  padding: 0 30rpx;
}
.main .signFor .contracttig {
  height: 100rpx;
  font-size: 18px;
  font-family: DINAlternate-Bold,DINAlternate;
  font-weight: bold;
  color: #333333;
  line-height: 100rpx;
  padding-top: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.main .signFor .contracttig .moreinfo {
  width: 100rpx;
  padding-right: 40rpx;
  position: relative;
  font-size: 14px;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #666666;
}
.main .signFor .contracttig .moreinfo image {
  height: 24rpx;
  width: 24rpx;
  position: absolute;
  top: 50%;
  right: 0rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.main .signFor .imagebox {
  height: 340rpx;
}
.main .signFor .imagebox .fahuocode {
  font-size: 16px;
  font-family: PingFangSC-Regular,PingFang SC;
  font-weight: 400;
  color: #333333;
}
.main .signFor .imagebox .fahuocode text {
  color: #0d80f7;
  text-decoration: underline;
  font-size: 12px;
  margin-left: 20rpx;
}
.main .signFor .imagebox .signImageBox {
  width: 100%;
  height: 200rpx;
